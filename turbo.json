{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "outputs": ["../../node_modules/.prisma/client"]
    },
    "db:migrate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:seed": {
      "inputs": ["prisma/schema.prisma", "prisma/seed.ts"],
      "cache": false
    },
    "db:reset": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:studio": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:deploy": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": [".next/**"]
    },
    "test": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "lint": {},
    "lint:fix": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^db:generate:client"],
      "cache": false
    }
  },
  "globalEnv": []
}
