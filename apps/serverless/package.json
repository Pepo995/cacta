{
  "name": "@cacta/serverless",
  "version": "1.0.0",
  "description": "Serverless app to host Cacta Worker",
  "license": "MIT",
  "main": "serverless.ts",
  "scripts": {
    "copy-prisma": "cp ./../../node_modules/.prisma/client/schema.prisma ./../../node_modules/.prisma/client/libquery_engine-* ./src/functions/worker",
    "deploy": "pnpm copy-prisma && serverless deploy",
    "dev": "pnpm copy-prisma && IS_OFFLINE=true pnpm with-env serverless offline start --stage local",
    "invoke": "IS_OFFLINE=true pnpm with-env serverless invoke local --function worker --stage local",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@cacta/db": "*"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.30.1",
    "@types/aws-lambda": "^8.10.123",
    "esbuild": "^0.19.4",
    "serverless": "^3.35.2",
    "serverless-esbuild": "^1.48.4",
    "serverless-offline": "^13.2.0",
    "serverless-prune-plugin": "^2.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
